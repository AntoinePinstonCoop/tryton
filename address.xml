<?xml version="1.0"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="address_view_form">
            <field name="model">relationship.address</field>
            <field name="inherit" ref="relationship.address_view_form"/>
            <field name="arch" type="xml">
                <![CDATA[
                <data>
                    <xpath
                        expr="/form/field[@name=&quot;email&quot;]"
                        position="after">
                        <newline/>
                        <label name="google_maps_url"/>
                        <field name="google_maps_url" widget="url"/>
                    </xpath>
                </data>
                ]]>
            </field>
        </record>
    </data>
</tryton>
