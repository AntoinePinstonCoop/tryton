<?xml version="1.0"?>
<tryton>
    <data>
        <record model="res.group" id="group_account">
            <field name="name">Account</field>
        </record>
        <record model="res.group" id="group_account_admin">
            <field name="name">Account admin</field>
        </record>
        <record model="res.user" id="res.user_admin">
